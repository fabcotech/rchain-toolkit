{
  "name": "rchain-toolkit",
  "version": "0.3.7",
  "description": "Node JS utils and methods to interact with rchain's rnode api and proto interfaces",
  "keywords": [
    "rchain",
    "rnode",
    "rholang",
    "blockchain",
    "dag",
    "smart contract",
    "grpc"
  ],
  "main": "dist/index.js",
  "scripts": {
    "test:jest": "jest",
    "test:full": "node test.js",
    "build:browser": "npx rollup -c rollup.config.iife.js",
    "build:node": "npx rollup -c rollup.config.cjs.js",
    "compile": "tsc --module commonjs --target es2020 src/index.ts --outDir dist -d && cp -r src/protobuf dist/ && cp src/rnode-protos* dist/",
    "compile:tests": "tsc tests/index.ts --outDir testsc && cp -r src/protobuf testsc/src/ && cp src/rnode-protos.js testsc/src/",
    "compile:examples": "rm examples/*.js && tsc examples/*"
  },
  "author": "FABCO",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-typescript": "^7.3.3",
    "@grpc/grpc-js": "^1.3.7",
    "@grpc/proto-loader": "^0.6.5",
    "@types/elliptic": "^6.4.13",
    "@types/node": "^12.0.8",
    "rollup": "1.26.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript": "^1.0.1",
    "ts-loader": "^6.0.3",
    "tslib": "^1.10.0",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "blakejs": "^1.1.0",
    "elliptic": "6.5.4",
    "keccak256": "^1.0.3",
    "protobufjs": "^6.11.2",
    "typescript": "3.6.4"
  }
}
